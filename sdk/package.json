    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": ">=0.30.0",
    "@solana/web3.js": ">=1.90.0"
